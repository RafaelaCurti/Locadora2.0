<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@(ViewBag.Title ?? "Locadora")</title>
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width">
    @H.Stylesheet("Bootstrap/bootstrap.min.css")
    <style>
        body {
            padding-top: 160px;
            padding-bottom: 100px;
        }
    </style>
    @H.Stylesheet("bootstrap/bootstrap-responsive.min.css")
    @H.Stylesheet("css/toastr.css")
    @H.Script("jquery-1.7.2.min.js")
    @H.Script("modernizr-2.5.3-respond-1.1.0.min.js")
    @RenderSection("header", required: false)

    @H.Stylesheet("css/bootstrap-custom.css")
</head>
<body>
    <!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                </a>
                <a class="brand" href="@Url.Action(MVC.Home.Index())">
                    <img class="logoImagem" src="~/Content/img/LocaFilme-sem-fundo.png" alt="Logo Locadora" />
                </a>
                <div class="nav-collapse">
                    <ul class="nav">
                        <li><a href="@Url.Action(MVC.Home.Index())">Home</a></li>
                        <li><a href="@Url.Action(MVC.Cliente.Home.Index())">Cliente</a> </li>
                        <li><a href="@Url.Action(MVC.Painel.Home.Index())">Usuário</a> </li>

                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    @*<hr>*@
    <div id="modal"></div>
    @Html.Partial("_alerta")
    <footer class="navbar navbar-fixed-bottom bg-info" id="footer-layout">
        <p class="footer-personalizado">
            &copy; Company 2020
        </p>
    </footer>
    @H.Script("bootstrap/bootstrap.min.js")
    @H.Script("jquery.livequery.min.js")
    @H.Script("toastr.js")
    @H.Script("master.js")
    @H.Script("jquery.validate.min.js")
    @H.Script("messages_pt_br.js")
    <script type="text/javascript">
        var _gaq = [['_setAccount', 'UA-XXXXX-X'], ['_trackPageview']];
        (function (d, t) {
            var g = d.createElement(t), s = d.getElementsByTagName(t)[0];
            g.src = ('https:' == location.protocol ? '//ssl' : '//www') + '.google-analytics.com/ga.js';
            s.parentNode.insertBefore(g, s)
        }(document, 'script'));
    </script>
</body>
</html>
